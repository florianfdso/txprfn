<?xml version="1.0"?>
<odoo>
    <record id="prep_model_tree" model="ir.ui.view">
        <field name="name">prep.product.tree</field>
        <field name="model">prep.product</field>
        <field name="arch" type="xml">
            <tree string="Channel" editable="bottom">
                <field name="name"/>
                <field name="description"/>
            </tree>            
        </field>    
    </record>

    <record id="prep_model_action" model="ir.actions.act_window">
        <field name="name">Product Name</field>
        <field name="res_model">prep.product</field>
        <field name="view_mode">tree,form</field>
        <field name="drawing" widget="many2many_binary"/>
    </record>
</odoo>